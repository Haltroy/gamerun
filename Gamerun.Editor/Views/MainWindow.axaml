    </Window.Resources>

    <Border Background="{DynamicResource SystemControlBackgroundAltMediumBrush}" CornerRadius="20">
        <DockPanel Margin="5">
            <DockPanel DockPanel.Dock="Top">
                <Panel PointerPressed="TopLeftPointerPressed" DockPanel.Dock="Left" Background="#00000001" Height="5"
                       Width="5" Cursor="TopLeftCorner" />
                <Panel PointerPressed="TopRightPointerPressed" DockPanel.Dock="Right" Background="#00000001" Height="5"
                       Width="5" Cursor="TopRightCorner" />
                <Panel PointerPressed="TopPointerPressed" Background="#00000001" Height="5" Cursor="SizeNorthSouth" />
            </DockPanel>
            <DockPanel DockPanel.Dock="Bottom">
                <Panel PointerPressed="BottomLeftPointerPressed" DockPanel.Dock="Left" Background="#00000001"
                       Height="5" Width="5" Cursor="BottomLeftCorner" />
                <Panel PointerPressed="BottomRightPointerPressed" DockPanel.Dock="Right" Background="#00000001"
                       Height="5" Width="5" Cursor="BottomRightCorner" />
                <Panel PointerPressed="BottomPointerPressed" Background="#00000001" Height="5" Cursor="SizeNorthSouth" />
            </DockPanel>
            <Panel PointerPressed="LeftPointerPressed" DockPanel.Dock="Left" Width="5" Background="#00000001"
                   Cursor="SizeWestEast" />
            <Panel PointerPressed="RightPointerPressed" DockPanel.Dock="Right" Width="5" Background="#00000001"
                   Cursor="SizeWestEast" />
            <DockPanel DockPanel.Dock="Top" PointerPressed="MoveTitleBar" Background="#00000001"
                       LastChildFill="True">
                <StackPanel DockPanel.Dock="Right" Orientation="Horizontal" Spacing="5">
                    <Button CornerRadius="10" Click="Minimize">_</Button>
                    <Button CornerRadius="10" Click="MaximizeNormal">
                        <Panel>
                            <PathIcon Data="{StaticResource maximize_regular}" Width="16" Height="16" />
                        </Panel>
                    </Button>
                    <Button Click="Close" CornerRadius="10">
                        <TextBlock Text="x" VerticalAlignment="Center" HorizontalAlignment="Center" />
                    </Button>
                </StackPanel>
                <ToggleButton CornerRadius="10" Name="SidebarShowHide" DockPanel.Dock="Left">
                    <Panel>
                        <PathIcon Data="{StaticResource pane_close_regular}"
                                  IsVisible="{CompiledBinding $parent[ToggleButton].IsChecked}" />
                        <PathIcon Data="{StaticResource pane_open_regular}"
                                  IsVisible="{CompiledBinding !$parent[ToggleButton].IsChecked}" />
                    </Panel>
                </ToggleButton>
                <TextBlock Text="{x:Static trans:Translations.GamerunEditorTitle}" VerticalAlignment="Center"
                           HorizontalAlignment="Center" />
            </DockPanel>
            <Grid ColumnDefinitions="Auto,*">
                <Border Grid.Column="0" Background="{DynamicResource SystemControlBackgroundChromeMediumBrush}"
                        CornerRadius="20" Margin="5">
                    <DockPanel LastChildFill="False">
                        <Button Click="SidebarButtonClicked" Tag="{CompiledBinding #AppList}" CornerRadius="10"
                                DockPanel.Dock="Top" Margin="5"
                                HorizontalAlignment="Stretch">
                            <Button.Content>
                                <StackPanel Orientation="Horizontal">
                                    <PathIcon Data="{StaticResource home_regular}" />
                                    <TextBlock Margin="5" VerticalAlignment="Center"
                                               Text="{x:Static trans:Translations.SidebarApps}"
                                               IsVisible="{CompiledBinding #SidebarShowHide.IsChecked}" />
                                </StackPanel>
                            </Button.Content>
                            <ToolTip.Tip>
                                <TextBlock Text="{x:Static trans:Translations.SidebarApps}" />
                            </ToolTip.Tip>
                        </Button>
                        <Button Click="SidebarButtonClicked" Tag="{CompiledBinding #ConfigMenu}" CornerRadius="10"
                                DockPanel.Dock="Top" Margin="5"
                                HorizontalAlignment="Stretch">
                            <Button.Content>
                                <StackPanel Orientation="Horizontal">
                                    <PathIcon Data="{StaticResource games_regular}" />
                                    <TextBlock Margin="5" VerticalAlignment="Center"
                                               Text="{x:Static trans:Translations.SidebarConfigs}"
                                               IsVisible="{CompiledBinding #SidebarShowHide.IsChecked}" />
                                </StackPanel>
                            </Button.Content>
                            <ToolTip.Tip>
                                <TextBlock Text="{x:Static trans:Translations.SidebarConfigs}" />
                            </ToolTip.Tip>
                        </Button>
                        <Button Click="SidebarButtonClicked" Tag="{CompiledBinding #MangoHudMenu}" CornerRadius="10"
                                DockPanel.Dock="Top" Margin="5"
                                HorizontalAlignment="Stretch">
                            <Button.Content>
                                <StackPanel Orientation="Horizontal">
                                    <PathIcon Data="{StaticResource fps_960_regular}" />
                                    <TextBlock Margin="5" VerticalAlignment="Center"
                                               Text="{x:Static trans:Translations.SidebarMangoHUD}"
                                               IsVisible="{CompiledBinding #SidebarShowHide.IsChecked}" />
                                </StackPanel>
                            </Button.Content>
                            <ToolTip.Tip>
                                <TextBlock Text="{x:Static trans:Translations.SidebarMangoHUD}" />
                            </ToolTip.Tip>
                        </Button>
                        <Button Click="SidebarButtonClicked" Tag="{CompiledBinding #StrangleMenu}" CornerRadius="10"
                                DockPanel.Dock="Top" Margin="5"
                                HorizontalAlignment="Stretch">
                            <Button.Content>
                                <StackPanel Orientation="Horizontal">
                                    <PathIcon Data="{StaticResource gavel_regular}" />
                                    <TextBlock Margin="5" VerticalAlignment="Center"
                                               Text="{x:Static trans:Translations.SidebarStrangle}"
                                               IsVisible="{CompiledBinding #SidebarShowHide.IsChecked}" />
                                </StackPanel>
                            </Button.Content>
                            <ToolTip.Tip>
                                <TextBlock Text="{x:Static trans:Translations.SidebarStrangle}" />
                            </ToolTip.Tip>
                        </Button>
                        <Button Click="SidebarButtonClicked" Tag="{CompiledBinding #GamescopeMenu}" CornerRadius="10"
                                DockPanel.Dock="Top" Margin="5"
                                HorizontalAlignment="Stretch">
                            <Button.Content>
                                <StackPanel Orientation="Horizontal">
                                    <PathIcon Data="{StaticResource window_multiple_regular}" />
                                    <TextBlock Margin="5" VerticalAlignment="Center"
                                               Text="{x:Static trans:Translations.SidebarGamescope}"
                                               IsVisible="{CompiledBinding #SidebarShowHide.IsChecked}" />
                                </StackPanel>
                            </Button.Content>
                            <ToolTip.Tip>
                                <TextBlock Text="{x:Static trans:Translations.SidebarGamescope}" />
                            </ToolTip.Tip>
                        </Button>
                        <Button Click="SidebarButtonClicked" Tag="{CompiledBinding #About}" CornerRadius="10"
                                DockPanel.Dock="Bottom" Margin="5"
                                HorizontalAlignment="Stretch">
                            <Button.Content>
                                <StackPanel Orientation="Horizontal">
                                    <PathIcon Data="{StaticResource info_regular}" />
                                    <TextBlock Margin="5" VerticalAlignment="Center"
                                               Text="{x:Static trans:Translations.SidebarAbout}"
                                               IsVisible="{CompiledBinding #SidebarShowHide.IsChecked}" />
                                </StackPanel>
                            </Button.Content>
                            <ToolTip.Tip>
                                <TextBlock Text="{x:Static trans:Translations.SidebarAbout}" />
                            </ToolTip.Tip>
                        </Button>
                        <Button Click="SidebarButtonClicked" Tag="{CompiledBinding #Settings}" CornerRadius="10"
                                DockPanel.Dock="Bottom" Margin="5"
                                HorizontalAlignment="Stretch">
                            <Button.Content>
                                <StackPanel Orientation="Horizontal">
                                    <PathIcon Data="{StaticResource settings_regular}" />
                                    <TextBlock Margin="5" VerticalAlignment="Center"
                                               Text="{x:Static trans:Translations.SidebarSettings}"
                                               IsVisible="{CompiledBinding #SidebarShowHide.IsChecked}" />
                                </StackPanel>
                            </Button.Content>
                            <ToolTip.Tip>
                                <TextBlock Text="{x:Static trans:Translations.SidebarSettings}" />
                            </ToolTip.Tip>
                        </Button>
                    </DockPanel>
                </Border>

                <Carousel Grid.Column="1" Name="ContentCarousel">
                    <Carousel.PageTransition>
                        <CrossFade Duration="0:0:0.2" />
                    </Carousel.PageTransition>
                    <views:AppList MainWindow="{CompiledBinding $parent[views:MainWindow]}" Name="AppList" />
                    <views:ConfigsMenu MainWindow="{CompiledBinding $parent[views:MainWindow]}" Name="ConfigMenu" />
                    <views:MangoHUDMenu MainWindow="{CompiledBinding $parent[views:MainWindow]}" Name="MangoHudMenu" />
                    <views:StrangleMenu MainWindow="{CompiledBinding $parent[views:MainWindow]}" Name="StrangleMenu" />
                    <views:GamescopeMenu MainWindow="{CompiledBinding $parent[views:MainWindow]}" Name="GamescopeMenu" />
                    <views:Settings MainWindow="{CompiledBinding $parent[views:MainWindow]}" Name="Settings" />
                    <views:AboutMenu MainWindow="{CompiledBinding $parent[views:MainWindow]}" Name="About" />
                </Carousel>
            </Grid>
        </DockPanel>
    </Border>
</Window>